{
  "name": "haus-2021",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.html",
  "scripts": {
    "dev": "parcel src/index.html --dist-dir ./docs/ --target default --public-url ./",
    "build": "npm run clean && parcel build src/index.html --dist-dir ./docs/ --target default --public-url ./",
    "clean": "rm -rf dist && rm -rf docs && rm -rf .parcel-cache"
  },
  "targets": {
    "default": {
      "distDir": "./docs",
      "publicUrl": "./"
    }
  },
  "staticFiles": {
    "distPath": "./docs/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moevbiz/haus-2021.git"
  },
