  "bugs": {
    "url": "https://github.com/moevbiz/haus-2021/issues"
  },
  "homepage": "https://github.com/moevbiz/haus-2021#readme",
  "devDependencies": {
    "parcel": "^2.0.0-beta.1",
    "parcel-reporter-static-files-copy": "^1.1.1",
    "sass": "^1.32.5"
  }
}
